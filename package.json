{
  "name": "yggdrasil",
  "version": "1.0.0",
  "description": "Front end to consume data from wiki-yggdrasil ruby gem",
  "main": "client/src/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "test",
    "build": "cd client && npm install && npm run build && cd ..",
    "deploy": "cp -a client/build/. public/",
