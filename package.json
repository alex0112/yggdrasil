    "start": "cd client/ && react-scripts start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alex0112/yggdrasil.git"
  },
  "keywords": [
    "spider",
    "wikipedia",
    "wiki-yggdrasil",
    "yggdrasil"
  ],
  "author": "Alex Larsen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alex0112/yggdrasil/issues"
  },
  "homepage": "https://github.com/alex0112/yggdrasil#readme",
  "dependencies": {
    "react-scripts": "^1.1.5"
  }
}
